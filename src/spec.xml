<?xml version="1.0" encoding="UTF-8"?>
<robot>
    <subsystems>

        <subsystem>
            <name>
                Drive
            </name>
            <motors>
                <motor>
                    <port>
                        0
                    </port>
                    <name>
                        driveRightMaster
                    </name>
                    <type>
                        TalonSRXMC
                    </type>
                    <sensor>AnalogEncoder</sensor>
                </motor>
                <motor>
                    <port>
                        1
                    </port>
                    <name>
                        driveRightSlave
                    </name>
                    <type>
                        TalonSRXMC
                    </type>
                    <sensor>NoSensor</sensor>
                </motor>
                <motor>
                    <port>
                        2
                    </port>
                    <name>
                        driveLeftMaster
                    </name>
                    <type>
                        TalonSRXMC
                    </type>
                    <sensor>AnalogEncoder</sensor>
                </motor>
                <motor>
                    <port>3</port>
                    <name>driveLeftSlave</name>
                    <type>TalonSRXMC</type>
                    <sensor>NoSensor</sensor>
                </motor>
                <motor>
                    <port>4</port>
                    <name>shifter</name>
                    <type>Solenoid</type>
                    <sensor>NoSensor</sensor>
                </motor>
            </motors>
            <actions>
                <action>
                    <name>setHighGear</name>
                    <type>Bumper_Right</type>
                    <m-motor>shifter</m-motor>
                    <m-sensor>NoSensor</m-sensor>
                    <threshold>0</threshold>
                    <controller>driverRemote</controller>
                    <value>true</value>
                    <reverse>false</reverse>
                </action>
            </actions>
            <algorithms>
                <algorithm>
                    <type>
                        DriveCheezyDriveEquation
                    </type>
                </algorithm>
            </algorithms>
        </subsystem>

        <subsystem>
            <name>FuelIntake</name>
            <motors>
                <motor>
                    <name>fuelIntake</name>
                    <type>SparkMC</type>
                    <port>7</port>
                    <sensor>NoSensor</sensor>
                </motor>
            </motors>
            <algorithms>
                <algorithm>
                    <type>BlankController</type>
                </algorithm>
            </algorithms>
            <actions>
                <action>
                    <m-motor>fuelIntake</m-motor>
                    <m-sensor>NoSensor</m-sensor>
                    <threshold>0</threshold>
                    <name>fuelIntakeIn</name>
                    <type>Trigger_Left</type>
                    <controller>operatorRemote</controller>
                    <reverse>false</reverse>
                </action>
                <action>
                    <m-motor>fuelIntake</m-motor>
                    <m-sensor>NoSensor</m-sensor>
                    <threshold>0</threshold>
                    <name>fuelIntakeOut</name>
                    <type>Trigger_Right</type>
                    <controller>operatorRemote</controller>
                    <reverse>true</reverse>
                </action>
            </actions>
        </subsystem>
        <subsystem>
            <name>GearIntake</name>
            <sensors>
                <sensor>
                    <name>gearHasSensor</name>
                    <type>AnalogInput</type>
                    <port>6</port>
                </sensor>
            </sensors>
            <motors>
                <motor>
                    <name>gearIntake</name>
                    <type>Solenoid</type>
                    <port>5</port>
                    <sensor>NoSensor</sensor>
                </motor>
            </motors>
            <actions>
                <action>
                    <m-motor>gearIntake</m-motor>
                    <m-sensor>NoSensor</m-sensor>
                    <threshold>0</threshold>
                    <type>Bumper_Right</type>
                    <controller>operatorRemote</controller>
                    <reverse>false</reverse>
                    <value>true</value>
                </action>
                <action>
                    <m-motor>NoMotor</m-motor>
                    <m-sensor>gearHasSensor</m-sensor>
                    <threshold>0.7</threshold>
                    <controller>operatorRemote</controller>
                    <reverse>false</reverse>
                    <value></value>
                </action>
            </actions>
        </subsystem>
    </subsystems>
    <controllers>
        <controller>
            <name>
                driverRemote
            </name>
            <port>
                0
            </port>
            <type>
                XboxController
            </type>
        </controller>
        <controller>
            <name>
                operatorRemote
            </name>
            <port>
                1
            </port>
            <type>
                XboxController
            </type>
        </controller>
    </controllers>
</robot>